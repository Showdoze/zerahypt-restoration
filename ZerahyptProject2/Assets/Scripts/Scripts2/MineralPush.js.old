var xspin = 0.0;
var yspin = 0.0;
var zspin = 0.0;
var Vel = 140.0;


function Start () {
		GetComponent.<Rigidbody>().AddRelativeForce(Vector3(Vel, 0, 0) * (GetComponent.<Rigidbody>().mass * 10));
		GetComponent.<Rigidbody>().angularVelocity  = Vector3(Random.Range(-0.1,0.1), Random.Range(-0.1,0.1), Random.Range(-0.1,0.1));
	}